<% include ../navbar.ejs%>
<div class="jumbotron">
    <div class="container">
        <a href="/crete-course" class="btn btn-primary btn-lg">Create a new course</a>

    </div>
</div>
<div class="container">
    <div class="row">
        <h2>Coursees that you teach</h2>
        <%for (var i=0;i<foundUser.coursesTeach.length;i++){%>
            <div class="col-md-4">
                <a href="/courses/<%=foundUser.coursesTeach[i]._id%>" style="text-decoration: none;color:34495e"></a>
                <div class="panel panel-default">
                    <div class="panel-body">
                <h3><%=foundUser.coursesTeach[i].title%></h3>
                <p><%=foundUser.coursesTeach[i].course.desc%></p>
                <a href="/edit-course/<%=foundUser.coursesTeach[i].course._id%>" class="btn btn-default">Edit Course</a>
            </div>
            </div>
            </div>
            <%}%>
    </div>
</div>